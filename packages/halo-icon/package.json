{
  "name": "@willcuii/halo-icon",
  "version": "0.0.5",
  "author": "Prenuvo Inc.",
  "description": "SVG Icon Library packages",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "metadata": [
        "./dist/metadata.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "rollup -c -w",
    "optimize": "svgo -f icons",
    "lint": "TIMING=1 eslint --cache ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./metadata": {
      "types": "./dist/metadata.d.ts",
      "import": "./dist/metadata.mjs",
      "default": "./dist/metadata.js"
    },
    "./dist/svg/*.svg": "./dist/svg/*.svg"
  },
  "publishConfig": {
    "access": "public",
    "@willcuii:registry": "https://npm.pkg.github.com"
  },
  "sideEffects": [
    "*.svg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willcuii/turborepo-design-library-template.git",
    "directory": "packages/halo-icon"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-virtual": "^3.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "@svgr/core": "^4.3.3",
    "globby": "^11.1.0",
    "hast-util-select": "^3.0.0",
    "rehype-parse": "^7.0.1",
    "rollup": "^3.12.0",
    "svgo": "^2.8.0",
    "unified": "^9.2.1"
  },
  "peerDependencies": {
    "react": "*"
  }
}